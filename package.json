{
  "name": "utf8-bin",
  "version": "0.3.1",
  "description": "A fast UTF-8 encoding and decoding",
  "keywords": ["utf8", "binary", "encoding", "decoding", "browser"],
  "license": "MIT",
  "author": {
    "name": "Alex Masterov",
    "email": "alex.masterow@gmail.com"
  },
  "homepage": "https://github.com/AlexMasterov/utf8-bin.js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexMasterov/utf8-bin.js.git"
  },
  "bugs": {
    "url": "https://github.com/AlexMasterov/utf8-bin.js/issues"
  },
  "main": "./src/index.js",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.esm.min.js",
  "files": ["src/", "dist/"],
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "nyc mocha test/*.test.js",
    "test-report": "nyc --reporter=html --reporter=text mocha test/*.test.js",
    "coverage": "nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "rollup": "0.67.0",
    "rollup-plugin-bundle-size": "1.0.2",
    "rollup-plugin-cjs-es": "0.7.0",
    "rollup-plugin-delete": "0.1.2",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-terser": "3.0.0"
  },
  "nyc": {
    "cache": false,
    "reporter": [],
    "include": ["src/**/*.js"]
  }
}
